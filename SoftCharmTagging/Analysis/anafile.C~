#include <iostream>
#include "TFile.h"
#include "TTree.h"

using namespace std;

void anafile(){
  
  TFile *file = TFile::Open("../datasets/CMS_MC_WZJetsTo3L1Nu_8TeV.root");   // Name of the file to open
  TTree *tree = (TTree*)file->Get("Events/Events");
  tree->MakeSelector("muonAna"); //Name of the .C & .h files
}


